{
  "name": "web-front-starter",
  "version": "0.1.0",
  "main": "dist/index.js",
  "author": "Lam Hieu <lamhieu.vk@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon app.js --ignore **/__test__",
    "build": "mode=production next build",
    "start": "mode=production babel-node app.js --ignore **/__test__ --presets env,es2017,stage-3",
    "clean": "rimraf build",
    "lint": "eslint --config .eslintrc app.js,api,pages,helpers,components,api --fix",
    "flow": "flow",
    "stats": "webpack-bundle-analyzer build/stats.json",
    "serve": "npm run clean && npm run lint && npm run build && npm run start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "nodejs",
    "express",
    "next",
    "front",
    "ssr",
    "react",
    "flow",
    "eslint",
    "es6",
    "source",
    "standard",
    "starter",
    "quickly"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "jest": "^22.4.2",
    "nodemon": "^1.17.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
